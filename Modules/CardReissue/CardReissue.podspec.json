{
  "name": "CardReissue",
  "authors": {
    "Alfa Mobile team": "alfabank.ci@gmail.com"
  },
  "description": "Модуль реализующий работу с картами",
  "homepage": "http://git/projects/IOSDEV/repos/am-ios/browse",
  "license": {
    "type": "Custom",
    "text": "Copyright 2018 - 2023\nPermission is granted to Alfa-Bank\n"
  },
  "platforms": {
    "ios": "12.0"
  },
  "script_phases": [
    {
      "name": "Timing START",
      "execution_position": "before_compile",
      "script": "if [ -f ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingSTART.sh ]; then\n  sh ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingSTART.sh\nfi\n"
    },
    {
      "name": "Timing END",
      "execution_position": "after_compile",
      "script": "if [ -f ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingEND.sh ]; then\n  sh ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingEND.sh\nfi\n"
    }
  ],
  "source": {
    "git": "ssh://git@git/iosdev/am-ios.git",
    "branch": null
  },
  "summary": "Модуль реализующий работу с картами",
  "swift_versions": "5.5",
  "version": "1.0",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": null
  },
  "dependencies": {
    "ABUIComponents": [

    ],
    "AlfaFoundation": [

    ],
    "AlfaNetworking": [

    ],
    "AMSharedProtocolsAndModels": [

    ],
    "CocoaLumberjack": [

    ],
    "NetworkKit": [

    ],
    "SharedRouter": [

    ],
    "SharedServices": [

    ]
  },
  "resource_bundles": {
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "dependencies": {
        "TestAdditions": [

        ]
      },
      "scheme": {
        "code_coverage": true,
        "environment_variables": {
          "IMAGE_DIFF_DIR": "$(SOURCE_ROOT)/FailedDiffs"
        },
        "launch_arguments": [
          "-AppleLanguages (en)",
          "-AppleLocale \"en_GB\""
        ]
      },
      "source_files": [
        "**/*+UITestable.swift",
        "**/*Snapshots.swift",
        "**/*Tests.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    }
  ],
  "subspecs": [
    {
      "name": "Generated",
      "source_files": [
        "**/Generated/**/*.swift",
        "**/*_gen.json"
      ],
      "exclude_files": [
        "**/Generated/**/*Tests.swift",
        "**/Generated/**/*Seeds.swift",
        "MiniApp"
      ]
    },
    {
      "name": "Mock",
      "source_files": [
        "**/*Mock.swift",
        "**/*Mocks.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    },
    {
      "name": "Seed",
      "source_files": [
        "**/*Seeds.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    },
    {
      "name": "Common",
      "source_files": "Common/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "Common/**/*+UITestable.swift",
        "Common/**/*_gen.json",
        "Common/**/*Mock.swift",
        "Common/**/*Mocks.swift",
        "Common/**/*Seeds.swift",
        "Common/**/*Snapshots.swift",
        "Common/**/*Tests.swift",
        "Common/**/Generated/**/*",
        "Common/MiniApp"
      ]
    },
    {
      "name": "OfficeChanging",
      "source_files": "OfficeChanging/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "OfficeChanging/**/*+UITestable.swift",
        "OfficeChanging/**/*_gen.json",
        "OfficeChanging/**/*Mock.swift",
        "OfficeChanging/**/*Mocks.swift",
        "OfficeChanging/**/*Seeds.swift",
        "OfficeChanging/**/*Snapshots.swift",
        "OfficeChanging/**/*Tests.swift",
        "OfficeChanging/**/Generated/**/*",
        "OfficeChanging/MiniApp"
      ]
    }
  ],
  "swift_version": "5.5"
}
