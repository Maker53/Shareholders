{
  "name": "Installments",
  "authors": {
    "Alfa Mobile team": "alfabank.ci@gmail.com"
  },
  "description": "Модуль рассрочек",
  "homepage": "http://git/projects/IOSDEV/repos/am-ios/browse",
  "license": {
    "type": "Custom",
    "text": "Copyright 2018 - 2023\nPermission is granted to Alfa-Bank\n"
  },
  "platforms": {
    "ios": "12.0"
  },
  "script_phases": [
    {
      "name": "Timing START",
      "execution_position": "before_compile",
      "script": "if [ -f ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingSTART.sh ]; then\n  sh ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingSTART.sh\nfi\n"
    },
    {
      "name": "Timing END",
      "execution_position": "after_compile",
      "script": "if [ -f ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingEND.sh ]; then\n  sh ../ciScripts/CompileTimeAnalytics/XcodeScripts/TimingEND.sh\nfi\n"
    }
  ],
  "source": {
    "git": "ssh://git@git/iosdev/am-ios.git",
    "branch": null
  },
  "summary": "Модуль со всеми рассрочками в проекте'",
  "swift_versions": "5.5",
  "version": "2.0.8",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": null
  },
  "dependencies": {
    "ABUIComponents": [

    ],
    "AlfaNetworking": [

    ],
    "IconPack/AMIconPack": [

    ],
    "IconPack/IconPack": [

    ],
    "OperationConfirmation": [

    ],
    "ResultScreen": [

    ],
    "SharedRouter": [

    ],
    "SnapKit": [

    ]
  },
  "resource_bundles": {
    "InstallmentsResources": "Resources/ModuleData/**"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "dependencies": {
        "TestAdditions": [

        ]
      },
      "scheme": {
        "code_coverage": true,
        "environment_variables": {
          "IMAGE_DIFF_DIR": "$(SOURCE_ROOT)/FailedDiffs"
        },
        "launch_arguments": [
          "-AppleLanguages (en)",
          "-AppleLocale \"en_GB\""
        ]
      },
      "source_files": [
        "**/*+UITestable.swift",
        "**/*Snapshots.swift",
        "**/*Tests.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    }
  ],
  "subspecs": [
    {
      "name": "Generated",
      "source_files": [
        "**/Generated/**/*.swift",
        "**/*_gen.json"
      ],
      "exclude_files": [
        "**/Generated/**/*Tests.swift",
        "**/Generated/**/*Seeds.swift",
        "MiniApp"
      ]
    },
    {
      "name": "Mock",
      "source_files": [
        "**/*Mock.swift",
        "**/*Mocks.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    },
    {
      "name": "Seed",
      "source_files": [
        "**/*Seeds.swift"
      ],
      "exclude_files": [
        "MiniApp"
      ]
    },
    {
      "name": "CancelInstallment",
      "source_files": "CancelInstallment/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "CancelInstallment/**/*+UITestable.swift",
        "CancelInstallment/**/*_gen.json",
        "CancelInstallment/**/*Mock.swift",
        "CancelInstallment/**/*Mocks.swift",
        "CancelInstallment/**/*Seeds.swift",
        "CancelInstallment/**/*Snapshots.swift",
        "CancelInstallment/**/*Tests.swift",
        "CancelInstallment/**/Generated/**/*",
        "CancelInstallment/MiniApp"
      ]
    },
    {
      "name": "Common",
      "source_files": "Common/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "Common/**/*+UITestable.swift",
        "Common/**/*_gen.json",
        "Common/**/*Mock.swift",
        "Common/**/*Mocks.swift",
        "Common/**/*Seeds.swift",
        "Common/**/*Snapshots.swift",
        "Common/**/*Tests.swift",
        "Common/**/Generated/**/*",
        "Common/MiniApp"
      ]
    },
    {
      "name": "InstalmentDetail",
      "source_files": "InstalmentDetail/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "InstalmentDetail/**/*+UITestable.swift",
        "InstalmentDetail/**/*_gen.json",
        "InstalmentDetail/**/*Mock.swift",
        "InstalmentDetail/**/*Mocks.swift",
        "InstalmentDetail/**/*Seeds.swift",
        "InstalmentDetail/**/*Snapshots.swift",
        "InstalmentDetail/**/*Tests.swift",
        "InstalmentDetail/**/Generated/**/*",
        "InstalmentDetail/MiniApp"
      ]
    },
    {
      "name": "InstalmentList",
      "source_files": "InstalmentList/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "InstalmentList/**/*+UITestable.swift",
        "InstalmentList/**/*_gen.json",
        "InstalmentList/**/*Mock.swift",
        "InstalmentList/**/*Mocks.swift",
        "InstalmentList/**/*Seeds.swift",
        "InstalmentList/**/*Snapshots.swift",
        "InstalmentList/**/*Tests.swift",
        "InstalmentList/**/Generated/**/*",
        "InstalmentList/MiniApp"
      ]
    },
    {
      "name": "Localization",
      "source_files": "Localization/**/*.{swift,h,m,mm,c}",
      "exclude_files": [
        "Localization/**/*+UITestable.swift",
        "Localization/**/*_gen.json",
        "Localization/**/*Mock.swift",
        "Localization/**/*Mocks.swift",
        "Localization/**/*Seeds.swift",
        "Localization/**/*Snapshots.swift",
        "Localization/**/*Tests.swift",
        "Localization/**/Generated/**/*",
        "Localization/MiniApp"
      ]
    }
  ],
  "swift_version": "5.5"
}
